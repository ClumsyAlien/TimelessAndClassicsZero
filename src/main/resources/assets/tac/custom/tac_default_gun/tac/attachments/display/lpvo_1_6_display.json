{
  // 配件在背包/快捷栏等容器中槽位显示的 2D 材质
  "slot": "tac:attachment/slot/lpvo_1_6",
  // 配件的默认模型，不可为空
  "model": "tac:attachment/lpvo_1_6_geo",
  // 配件默认模型使用的材质，不可为空
  "texture": "tac:attachment/uv/lpvo_1_6",
  // 只有 瞄具 配件需要设置这个属性，表示瞄具的放大倍率。
  // 玩家可以在数组设置的几个倍率之间切换
  "zoom": [3.0, 6.0],
  // 只有 瞄具 配件需要设置这个属性，表示开镜后，枪身和瞄具渲染的fov。
  // 默认情况下，mc 渲染手部模型的 fov 为 70。
  "fov": 70.0
}